{
  "ignore": {
    "cord_loader": true,
    "downloader": true,
    "text_loader":true,
    "splitter": true,
    "ner": true,
    "analysis": true,
    "merger": true,
    "metrics":false,
	"nel":true
  },
  "downloader": {
    "input_path": "data/Lund-Autophagy-1.txt",
    "output_path": "data/Lund-Autophagy-1.json",
    "batch_size": 100 
  },
  "cord_loader": {
    "input_path": "data/path-to-cord/metadata.csv",
    "output_path": "data/path-to-cord/metadata.json",
	"subset":false,
	"subset_file": ""
  },
    "text_loader": {
    "input_path": "data/path-to-freetext/sample_text.txt",
    "output_path": "data//path-to-freetext/freetext.json",
	"title":"",
	"id": "freetext"
  },
  "splitter": {
    "input_path": "data/Lund-Autophagy-1.json",
	"output_folder": "data/sentencer/text-sentences_Lund-Autophagy-1/",
    "output_file_prefix": "text-sentences_Lund-Autophagy-1",
    "tokenizer": "spacy",
    "model_name": "en_core_web_sm",
	"batch_size": 100
  },
  "ner": {
    "input_path": "data/sentencer/text-sentences_Lund-Autophagy-1/",
    "output_path": "results/text-ner-Lund-Autophagy-1_species/",
	  "output_file_prefix": "text-ner-Lund-Autophagy-1_species",
    "model_type": "biobert_finetuned",
    "model_folder": "aitslab",
    "model_name": "biobert_huner_species_v1",
    "vocab_path": "",
    "store_tokens":"no",
    "labels": "",
    "clear_old_results": true,
    "article_limit": [-1,90000],
	  "entity_type": "species",
	  "multiprocessing":true
  },
  "analysis": {
    "input_path": "results/text-ner-Lund-Autophagy-1_species/",
    "output_path": "results/"
  },
  "merger": {
	  "paths": ["path/to/model-1/", "path/to/model-2/", "path/to/model-3/"],
	  "entities": ["model-1_entity", "model-2_entity","model-3_entity"],
	  "output_path": "results/merged_sample.json"
  },
  "metrics": {
    "predictions_file":"path/to/predictions/file.txt",
    "true_file":"path/to/true/annotated/file.txt",
    "output_file":"path/to/output/file.txt"
  },
  "nel":{
	  "input_path": "results/text-ner-Lund-Autophagy-1_species/",
	  "output_path": "results/text-nel_Lund_Autophagy-1_species.json"
  }
}
